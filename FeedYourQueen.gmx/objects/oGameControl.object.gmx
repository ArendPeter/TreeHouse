        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(room != rmGrass)
    exit;
    
if(instance_exists(oPlayer) &amp;&amp; !oQueen.dying){
    targetX = oPlayer.x - view_wview/2 + sprite_width/2;
    targetY = oPlayer.y - view_hview/2 + sprite_height/2;
    tweenFactor = .1;
    if(keyboard_check(ord('R'))) tweenFactor = .6;
}else if(oQueen.finalScene || oQueen.dying){
    targetX = oQueen.x - view_wview / 2;
    targetY = oQueen.y - view_hview / 2;
    tweenFactor = .2;
}else{
    targetX = oQueen.x - view_wview / 4;
    targetY = oQueen.y - view_hview / 2;
    tweenFactor = .2;
}
updateCamera();

var viewDist = point_distance(view_xview,view_yview,targetX,targetY);
cameraOnQueen = (!instance_exists(oPlayer) || oQueen.dying) &amp;&amp; viewDist &lt; 10;

if(!instance_exists(oPlayer)){
    playerSpawnTime = NULL;
}else if(playerSpawnTime == NULL){
    playerSpawnTime = current_time;
}

if(oGameControl.debug &amp;&amp; !titleScreen &amp;&amp; keyboard_check_pressed(vk_space))
    game_restart();
    
if(titleScreen &amp;&amp; (keyboard_check_pressed(vk_anykey) || mouse_check_button(mb_left))){
    titleScreen = false;
}

if(keyboard_check_pressed(ord('F'))){
    fullscreen = !fullscreen;
    window_set_fullscreen(fullscreen);
}


